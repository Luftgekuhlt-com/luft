 <%= simple_form_for @document, { url: [:admin, @document] } do |f| %>
    <h4 class="modal-title">New Page</h4>
    <%= f.error_notification %>

    <div class="row">
      <%= f.input :title, wrapper_html: { class: "col m12" }  %>
    </div>
    <div class="modal-footer">
        <%= f.button :button, "Cancel", class: "btn modal-close"%>
        <%= f.button :submit, "Save", class: "btn btn-primary btn-submit", data: { disable_with: "Save" } %>
    </div>
  <% end %>